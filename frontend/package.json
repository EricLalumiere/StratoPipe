{
  "name": "stratopipe-frontend",
  "version": "1.0.0",
  "private": true,
  "proxy": "http://localhost:8000",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^7.0.0",
    "axios": "^1.2.1",
    "esbuild": "0.25.9",
    "npm-run-all": "^4.1.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "^6.9.0"
  },
  "overrides": {
    "nth-check": "^2.1.1",
    "postcss": "^8.4.33",
    "svgo": "^2.8.0",
    "@svgr/plugin-svgo": "^7.0.0",
    "@svgr/webpack": "^8.0.1",
    "webpack-dev-server": "^5.0.4"
  },
  "scripts": {
    "build": "npm run build:pm && npm run build:project",
    "build:pm": "webpack --config webpack.pm.config.js",
    "build:create-project": "esbuild public/entries/create-project.entry.js --bundle --outfile=public/dist/create-project.bundle.js --format=iife --platform=browser --define:process.env.REACT_APP_API_URL='\"http://localhost:8000/api/\"'",
    "build:pages": "npm run build:pm && npm run build:create-project",
    "watch:pm": "esbuild public/entries/pm.entry.js --bundle --outfile=public/dist/pm.bundle.js --format=iife --platform=browser --watch --define:process.env.REACT_APP_API_URL='\"http://localhost:8000/api/\"'",
    "watch:create-project": "esbuild public/entries/create-project.entry.js --bundle --outfile=public/dist/create-project.bundle.js --format=iife --platform=browser --watch --define:process.env.REACT_APP_API_URL='\"http://localhost:8000/api/\"'",
    "serve:public": "python3 -m http.server 3000 -d public",
    "dev": "npm-run-all -p watch:* serve:public",
    "build:project": "webpack --config webpack.project.config.js",
    "start": "webpack serve --config webpack.project.config.js --open --hot --port 3000",
    "start:pm": "webpack serve --config webpack.pm.config.js --open --hot --port 3001"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-react": "^7.24.7",
    "babel-loader": "^9.2.1",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.2"
  }
}
